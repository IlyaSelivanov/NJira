@model NJira.WebUI.Models.TransactionHistoryViewModel

@{
    ViewBag.Title = "History";
}

<h2>History</h2>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Key</th>
            <th>Summary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var line in Model.Transactions)
            {
            <tr>
                <td>@line.Date</td>
                <td>@line.Key</td>
                <td>@line.Summary</td>
            </tr>
        }
    </tbody>
</table>

